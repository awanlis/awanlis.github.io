{
    "Flash Sales": {
      "metadata": {
        "metrics": ["TY Sales Quantity, TY Sales Amount, TY Profit Amount, LY Sales Quantity, LY Sales Amount, LY Profit Amount"],
        "attributes": ["department"],
        "filter":{
          "year":"2021",
          "store":"365"
        }
      },
      "permission": {
        "roles": [],
        "users": []
      }
    },
    "Store Daily Sales and Profit": {
      "metadata": {
        "metrics": ["BOH", "Sales Quantity", "Sales Amount", "EOH", "Profit Amount"],
        "attributes": ["department"],
        "filter": {
          "day": "yesterday"
        }
      },
      "permission": {
        "roles": [],
        "users": []
      }
    },
    "Inventory Movement": {
      "metadata": {
        "metrics": ["BOH", "EOH","Sales Quantity", "Inventory Received Quantity", "RTV Quantity", "Inventory Transfer"],
        "attributes": ["department"],
        "filter":{
          "year":"2021",
          "store":"365"
        }
      },
      "permission": {
        "roles": [],
        "users": []
      }
    },
    "Inventory Status": {
      "metadata":{
        "metrics":["EOH","BOH","sales"],
        "attributes":["department"],
        "filter":{
          "year":"2021",
          "store":"365"
        }
      }
    },
    "Types of Sales": {
      "metadata":{
